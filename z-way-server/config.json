{
  "name": "Z-Way Server",
  "version": "0.0.1",
  "slug": "z-way-server",
  "description": "Z-Way ZWave server instance",
  "url": "https://github.com/johnpdowling/hassio-addons/z-way-server",
  "startup": "before",
  "auto_uart": true,
  "ingress": true,
  "ingress_port": 8083,
  "ingress_entry": "smarthome/#/",
  "panel_icon": "mdi:z-wave",
  "arch": [
    "aarch64",
    "armhf",
    "armv7"
  ],
  "boot": "auto",
  "map": [ 
    "config:rw",
    "ssl"
  ],
  "options": {
    "debug": false,
    "ssl": false
  },
  "schema": {
    "debug": "bool",
    "ssl": "bool"
  }
}
