{
  "name": "Keras REST Server",
  "version": "0.0.1",
  "slug": "keras-rest-server",
  "description": "Keras REST server instance",
  "url": "https://github.com/johnpdowling/hassio-addons/keras-rest",
  "startup": "services",
  "ingress": false,
  "arch": [
    "aarch64"
  ],
  "boot": "auto",
  "ports": {
    "80/tcp": 5000
  },
  "ports_description": {
    "80/tcp": "REST interface"
  },
  "map": [ 
    "config:rw"
  ],
  "options": {
    "model_name": "imagenet"
  },
  "schema": {
    "model_name": "str"
  }
}
