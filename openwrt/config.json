{
  "name": "OpenWRT",
  "version": "0.0.1",
  "slug": "openwrt",
  "description": "OpenWRT instance",
  "url": "https://github.com/johnpdowling/hassio-addons/openwrt",
  "startup": "services",
  "privileged": [ "NET_ADMIN" ],
  "ingress": false,
  "host_network": false,
  "host_dbus": false,
  "ports": {
    "80/tcp": 1980
  },
  "ports_description": {
    "80/tcp": "Web interface"
  },
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "map": [ 
    "config:rw"
  ],
  "devices": [ "/sys/class/net/wlan0:/sys/class/net/wlan0:rwm" ],
  "options": {
  },
  "schema": {
  }
}
